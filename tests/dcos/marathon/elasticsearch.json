{
  "id": "elasticsearch",
  "instances": 1,
  "cpus": 1,
  "mem": 2048.0,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "magneticio/elastic:2.2",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 5601,
          "hostPort": 5601
        },
        {
          "containerPort": 9200,
          "hostPort": 9200
        }
      ],
      "forcePullImage": true
    }
  },
  "labels": {
    "DCOS_SERVICE_NAME": "kibana",
    "DCOS_SERVICE_SCHEME": "http",
    "DCOS_SERVICE_PORT_INDEX": "0"
  },
  "acceptedResourceRoles": [
    "slave_public"
  ],
  "healthChecks": [
    {
      "protocol": "TCP",
      "gracePeriodSeconds": 30,
      "intervalSeconds": 10,
      "timeoutSeconds": 5,
      "port": 9200,
      "maxConsecutiveFailures": 0
    }
  ]
}
